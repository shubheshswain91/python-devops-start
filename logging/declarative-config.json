{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": " %(levelname)8s - %(message)s"
        },
        "detailed": {
            "format": "%(asctime)s %(name)s [%(levelname)s]: %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "detailed",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "config.json": {
            "handlers": ["console"],
            "level": "DEBUG",
            "propagate": false   
        }
    },
    "root": {
        "handlers": ["console"],
        "level": "DEBUG"
    }
} 